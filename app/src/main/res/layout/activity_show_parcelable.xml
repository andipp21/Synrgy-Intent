<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShowParcelable">

    <TextView
        android:id="@+id/tvShowParcel"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PARCELABLE INTENT"
        android:textStyle="bold"
        android:textSize="18sp"
        android:layout_marginTop="16dp"/>

    <TextView
        android:id="@+id/tvShowNama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tvShowParcel"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="64dp"
        android:text="Nama : "
        android:layout_marginStart="32dp"
        android:textSize="24sp"/>

    <TextView
        android:id="@+id/tvShowNamaParcel"
        app:layout_constraintStart_toEndOf="@id/tvShowNama"
        app:layout_constraintBottom_toBottomOf="@id/tvShowNama"
        android:textSize="24sp"
        android:layout_width="192dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/tvShowUmur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tvShowNama"
        app:layout_constraintEnd_toEndOf="@id/tvShowNama"
        android:layout_marginTop="32dp"
        android:layout_marginStart="32dp"
        android:text="Umur : "
        android:textSize="24sp"/>

    <TextView
        android:id="@+id/tvShowUmurParcel"
        app:layout_constraintStart_toEndOf="@id/tvShowUmur"
        app:layout_constraintBottom_toBottomOf="@id/tvShowUmur"
        android:textSize="24sp"
        android:layout_width="192dp"
        android:layout_height="wrap_content"/>

</androidx.constraintlayout.widget.ConstraintLayout>